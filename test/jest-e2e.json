{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@lib(|/.*)$": "<rootDir>/lib/$1",
    "^src(|/.*)$": "<rootDir>/$1",
    "^decorator(|/.*)$": "<rootDir>/decorator/$1"
  }
}
